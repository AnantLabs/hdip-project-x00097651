        </td>
         <td>
            @Html.DisplayFor(modelItem => item.address2)
        </td>

        <td>
            @Html.DisplayFor(modelItem => item.occupied)
        </td>



        @*<td>
            @Html.DisplayFor(modelItem => item.Timestamp)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PartitionKey)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RowKey)
        </td>*@
        <td>
            @*We send PartitionKey and RowKey values as parameters in the URL*@
            @Html.ActionLink("Edit", "Edit", new { PartitionKey = item.PartitionKey, RowKey = item.RowKey /* id=item.PrimaryKey */ }) |
            @Html.ActionLink("Details", "Details", new { PartitionKey = item.PartitionKey, RowKey = item.RowKey /* id=item.PrimaryKey */ }) |
            @Html.ActionLink("Delete", "Delete", new { PartitionKey = item.PartitionKey, RowKey = item.RowKey /* id=item.PrimaryKey */ })
