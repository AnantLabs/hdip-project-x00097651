@model IEnumerable<MVC3WebRole.Models.Property>

@{
    ViewBag.Title = "Index";
}

<h2>List of Properties</h2>

<p>
    @Html.ActionLink("Add a new Property", "Create")
</p>
<table>
    <tr>
        <th>
           Property Id
        </th>
        <th>
           Address1
        </th>
        <th>
           
        </th>
        <th>
            Occupied
        </th>
        

        @*<th>
            Timestamp
        </th>
        <th>
            PartitionKey
        </th>
        <th>
            RowKey
        </th>*@
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
       
